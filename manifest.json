{
    "name": "TLDR QuickText",
    "version": "1",
    "manifest_version": 2,
    "description": "TLDR?  Read it faster",
    "browser_action": {
        "name": "TLDR",
        "icons": ["icon.png"],
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
	    "css": ["jquery-ui.min.css", "background.css"],
            "js": [ "jquery.min.js", "jquery-ui.min.js", "background.js" ],
            "matches": [ "http://*/*", "https://*/*"]
        }
    ]
}
